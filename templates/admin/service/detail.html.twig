{% extends 'admin/service/base.html.twig' %}

{% block title %}
    Détail du service
{% endblock %}
{% block stylesheet %}
    <style>
        select> option {
            color: black !important;
        }
        .invalid-feedback > ul li {
            /* display: none; */
            width: 100%;
            margin-top: 0.25rem;
            font-size: 80%;
            color: #f44336;
        }
    </style>
{% endblock %}
{% block content %}

    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header card-header-primary">
                    <h4 class="card-title "> Détail du servic</h4>
                    <p class="card-category"> </p>
                </div>
                <div class="card-body">
                    <h4 style="font-weight: bold">Titre</h4>
                    <p>{{ service.name }}</p>
                    <h4 style="font-weight: bold">Description</h4>
                    <p>{{ service.description }}</p>
                    <h4 style="font-weight: bold">Numéro vert</h4>
                    <p>{{ service.numVert }}</p>
                    <h4 style="font-weight: bold">Zone Dispo</h4>
                    <p>{{ service.zoneDispo }}</p>





                </div>
            </div>
        </div>
    </div>



{% endblock %}
{% block javascripts %}
{% endblock %}

